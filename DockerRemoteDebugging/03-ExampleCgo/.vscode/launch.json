{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Go",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "/app",
      "port": 4000,
      "host": "127.0.0.1",
      "showLog": true
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/stevepro/GitHub/StevePro7/CloudTesting/DockerRemoteDebugging/03-ExampleCgo",
      "program": "/home/stevepro/GitHub/StevePro7/CloudTesting/DockerRemoteDebugging/03-ExampleCgo/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}